  file = {Snapshot:/home/tehdog/.zotero/zotero/h74c1nl7.default/zotero/storage/7CKQ8K77/abstract.html:;Full Text PDF:/home/tehdog/.zotero/zotero/h74c1nl7.default/zotero/storage/VZMHNXKB/Weber et al. - 2009 - Interactive Geometric Simulation of 4D Cities.pdf:application/pdf}
