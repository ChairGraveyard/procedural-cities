  date = {2011-04-01},
  pages = {345--354},
  langid = {english},
  keywords = {I.3.5 [Computer Graphics]: Computational Geometry and Object Modeling,I.3.7 [Computer Graphics]: Three-dimensional Graphics and Realism},
  file = {Full Text PDF:/home/tehdog/.zotero/zotero/h74c1nl7.default/zotero/storage/3B6X97BI/Lipp et al. - 2011 - Interactive Modeling of City Layouts using Layers .pdf:application/pdf;Snapshot:/home/tehdog/.zotero/zotero/h74c1nl7.default/zotero/storage/W7B8WKUI/full.html:}
